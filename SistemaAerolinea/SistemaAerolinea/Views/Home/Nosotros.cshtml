
@{
    ViewData["Title"] = "Nosotros";
}
@section css {
    <link rel="stylesheet" href="~/css/nosotros.css" />
}

<h1 class="titulo">Integrantes</h1>


<div class="nosotros row justify-content-center">
    <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-4 d-flex">
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <i class="profile fa fa-user"></i>
                    <p>Santiago Frias</p>
                </div>
                <div class="flip-card-back">
                    <img src="/img/santiago.jpg" alt="Santiago Frias">
                </div>
            </div>
        </div>
    </div>
    <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-4 d-flex">
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <i class="profile fa fa-user"></i>
                    <p>Martin Gentile</p>
                </div>
                <div class="flip-card-back">
                    <img src="/img/martin.jpg" alt="Martin Gentile">
                </div>
            </div>
        </div>
    </div>
    <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-4 d-flex">
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <i class="profile fa fa-user"></i>
                    <p>Alan Buengueroff</p>
                </div>
                <div class="flip-card-back">
                    <img src="/img/alan.jpg" alt="Alan Buengueroff">
                </div>
            </div>
        </div>
    </div>
    <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-4 d-flex">
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <i class="profile fa fa-user"></i>
                    <p>Miguel Pinella</p>
                </div>
                <div class="flip-card-back">
                    <img src="/img/miguel.jpg" alt="Miguel Pinella">
                </div>
            </div>
        </div>
    </div>
    <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-4 d-flex">
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <i class="profile fa fa-user"></i>
                    <p>Juan Lamanna</p>
                </div>
                <div class="flip-card-back">
                    <img src="/img/juan.jpg" alt="Juan Lamanna">
                </div>
            </div>
        </div>
    </div>
</div>
